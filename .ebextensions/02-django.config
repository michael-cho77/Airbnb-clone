container_commands:
  01_migrate:
    command: "django-admin migrate"
    leader_only: true
  02_compilemessages:
    command: "django-admin compilemessages"
  03_createsu:
    command: "django-admin createsu"
  04_collectstatic:
    command: "django-admin collectstatic --noinput"
  05_seed:
    command: "django-admin seed_users --number 50 && django-admin seed_facilities && django-admin seed_amenities && django-admin seed_rooms --number 150 && django-admin seed_list && django-admin seed_reviews --number 500 && django-admin seed_avatars"
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: config/wsgi.py
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: config.settings